# Note: Buildroot's and Meson's terminologies differ about the meaning
# of 'build', 'host' and 'target':
# - Buildroot's 'host' is Meson's 'build'
# - Buildroot's 'target' is Meson's 'host'

[binaries]
c = '/home/fabian/Downloads/nios2--glibc--stable-2021.11-1/bin/nios2-buildroot-linux-gnu-gcc'
cpp = '/home/fabian/Downloads/nios2--glibc--stable-2021.11-1/bin/nios2-buildroot-linux-gnu-g++'
ar = '/home/fabian/Downloads/nios2--glibc--stable-2021.11-1/bin/nios2-buildroot-linux-gnu-ar'
strip = '/home/fabian/Downloads/nios2--glibc--stable-2021.11-1/bin/nios2-buildroot-linux-gnu-strip'
pkgconfig = '/home/fabian/Downloads/nios2--glibc--stable-2021.11-1/bin/pkgconf'
g-ir-compiler = '/home/fabian/Downloads/nios2--glibc--stable-2021.11-1/nios2-buildroot-linux-gnu/sysroot/usr/bin/g-ir-compiler'
g-ir-scanner = '/home/fabian/Downloads/nios2--glibc--stable-2021.11-1/nios2-buildroot-linux-gnu/sysroot/usr/bin/g-ir-scanner'

[built-in options]
c_args = ['-D_LARGEFILE_SOURCE', '-D_LARGEFILE64_SOURCE', '-D_FILE_OFFSET_BITS=64', '-Os', '-g0', '-D_FORTIFY_SOURCE=1'@PKG_TARGET_CFLAGS@]
c_link_args = [@PKG_TARGET_CFLAGS@]
cpp_args = ['-D_LARGEFILE_SOURCE', '-D_LARGEFILE64_SOURCE', '-D_FILE_OFFSET_BITS=64', '-Os', '-g0', '-D_FORTIFY_SOURCE=1'@PKG_TARGET_CFLAGS@]
cpp_link_args = [@PKG_TARGET_CFLAGS@]
wrap_mode = 'nodownload'

[properties]
needs_exe_wrapper = true
sys_root = '/home/fabian/Downloads/nios2--glibc--stable-2021.11-1/nios2-buildroot-linux-gnu/sysroot'
pkg_config_libdir = '/home/fabian/Downloads/nios2--glibc--stable-2021.11-1/nios2-buildroot-linux-gnu/sysroot/usr/lib/pkgconfig:/home/fabian/Downloads/nios2--glibc--stable-2021.11-1/nios2-buildroot-linux-gnu/sysroot/usr/share/pkgconfig'
pkg_config_static = 'false'

[host_machine]
system = 'linux'
cpu_family = 'nios2'
cpu = ''
endian = 'little'
